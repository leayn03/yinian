{
  "version": 2,
  "public": true,
  "rewrites": [
    { "source": "/(.*)", "destination": "/omikuji/$1" }
  ],
  "headers": [
    {
      "source": "/omikuji/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/omikuji/(css|js)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
